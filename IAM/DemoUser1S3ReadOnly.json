{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "S3ReadOnlyDemoUser1",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject"
      ],
      "Resource": [
        "arn:aws:s3:::demouser1-test-bucket",
        "arn:aws:s3:::demouser1-test-bucket/*"
      ],
      "Condition": {
        "StringEquals": {
          "aws:RequestedRegion": "us-east-1"
        }
      }
    }
  ]
}
